gpos:
- id: '{GPOId}'
  name: GPOName
  rules:
    dconf:
    - key: path/to/Gpo1key1
      value: ValueOfGpo1Key1
      meta: s
    - key: path/to/Gpo1key2
      value: ValueOfGpo1Key2
      meta: s
    scripts:
    - key: path/to/Gpo1key3
      disabled: true
- id: '{GPOId2}'
  name: GPOName2
  rules:
    dconf:
    - key: path/to/Gpo2key1
      value: ValueOfKey1
      meta: s
